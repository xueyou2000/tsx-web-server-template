{
  "name": "tsx-web-server-template",
  "version": "1.0.0",
  "description": "A modern TypeScript project template using ESM for web server",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "lint": "biome check .",
    "lint:unsafe-fix": "biome check --fix --unsafe .",
    "format": "biome format --write .",
    "format:fix": "biome format --write . && biome check --fix --unsafe .",
    "commitlint": "commitlint",
    "prepare": "husky install",
    "build": "tsc",
    "dev": "cross-env NODE_ENV=development tsx watch src/index.ts",
    "test": "cross-env NODE_ENV=test tsx src/index.ts",
    "prod": "cross-env NODE_ENV=production node dist/index.js"
  },
  "keywords": [
    "typescript",
    "esm",
    "tsx",
    "template"
  ],
  "author": "XueYou",
  "license": "ISC",
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/env": "^5.0.2",
    "@fastify/formbody": "^8.0.2",
    "@fastify/multipart": "^9.0.3",
    "@fastify/sensible": "^6.0.3",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "dotenv": "^16.3.1",
    "fastify": "^5.5.0",
    "fastify-plugin": "^5.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@types/node": "^20.11.10",
    "cross-env": "^10.0.0",
    "husky": "^8.0.0",
    "pino-pretty": "^13.1.1",
    "pino-roll": "^3.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
